<div class="row">
  <div class="col-lg-12">
    <h2 class="page-header">Featured Agents</h2>
  </div>
  <% Agent.all.each do |agent| %>
    <div class="col-md-2 col-sm-4 col-xs-6">
      <div class="featured_agent">
        <img class="img-responsive customer-img"  width="100" height="100" src=<%=agent.img_url%> alt="">
        <span class="caption">
          <%=link_to agent.name, agent.url %>
          <br><small>(<%=link_to agent.company, agent.url %>)</small>
        </span>
      </div>
    </div>
  <% end %>
</div>